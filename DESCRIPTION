Package: MorphSim
Type: Package
Title: Simulate Discrete Character Data along Phylogenetic Trees
Version: 0.1.0
Authors@R: c(person(given = "Laura", family = "Mulvey", role = c("aut", "cre", "cph"), email = "lauramulvey479@gmail.com"),
